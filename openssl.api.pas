//https://opensource.apple.com/source/xnu/xnu-201/osfmk/kern/sscanf.c.auto.html
function sscanf(str, format : PUTF8Char; args: array of const):integer;
var
    start    : PUTF8Char;
    positive,
    value    : integer;
    valp     : PInteger;
begin
  start := str;

  while format^ <> #0 do
  begin
    if (format^ = '%')  and  (format[1] = 'd') then
    begin
      if str^ = '-' then
      begin
        positive := 0;
        Inc(str);
      end
      else
        positive := 1;
      if  not isdigit(str^) then
          break;
      value := 0;
      repeat
        value := (value * 10) - ( Ord(str^) - Ord('0') );
        Inc(str);
      until not isdigit(str^);
      if positive > 0 then
         value := -value;
      valp := va_arg(args, TypeInfo(PInteger) );
      valp^ := value;
      Inc(format);
    end
    else
    if ( format^ = str^) then
    begin
      Inc(str);
    end
    else
      break;
    Inc(format) ;
  end;

  Result := str - start;
end;

