function strncpy(dst : PWideChar;const src : PWideChar; n : size_t):PWideChar;
var
  d, s : PWideChar;
  function IsZero: Boolean;
  begin
     d^ := s^;
     if d^ = #0 then
        Result := True
     else
        Result := False;
      Inc(d);
      Inc(s);
  end;
begin

  if n <> 0 then
  begin
    d := dst;
    s := src;

    repeat
      if IsZero then
      begin

        while PreDec(n) <> 0 do
        begin
          d^ := #0;
          Inc(d);

        end;
        break;

      end;
    until PreDec(n) = 0;

  end;
  Result := (dst);
end;

