function strcasecmp(const s1_arg, s2_arg : PUTF8Char):integer;
var
  s1, s2 : PUTF8Char;
begin
  if (s1_arg = nil) or (s2_arg = nil) then
     exit(-1);
  s1 := (s1_arg);
  s2 := (s2_arg);
  while tolower( s1^) = tolower( s2^) do
  begin
    if (s1^ = #0) then
		  	exit (0);
    Inc(s1);Inc(s2);
  end;
  Result := get_result(Uint32(tolower(s1^)) < Uint32(tolower(s2^)), -1 , +1);

end;


