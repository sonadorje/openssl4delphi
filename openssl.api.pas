Function isalnum(ch : AnsiChar) : Boolean;
Begin
  Result := isalpha(ch) or isdigit(ch);
End;

Function iscntrl(ch : AnsiChar) : Boolean;
Begin
  Result := ch in [#$00..#$1f,#$7f];
End;

