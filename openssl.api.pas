function strrchr( s : PUTF8Char; c : UTF8Char):PUTF8Char;
var
  ch : UTF8Char;
  sc : PUTF8Char;
begin
  { find last occurrence of c in char s[] }
   ch := (c);
  sc := nil;//0;
  while true do
  begin   { check another PUTF8Char }
    if s^ = ch then
       sc := s;
    if s^ = #0 then
       Exit(sc);
    Inc(s);
  end;
end;

