function strncmp(s1, s2 : PUTF8Char; n : size_t):integer;
var
  u1, u2 : byte;
begin
  while PostDec(n) > 0 do
    begin
      u1 := byte(s1^);
      Inc(s1);
      u2 := byte(s2^);
      Inc(s2);
      if u1 <> u2 then
        Exit(u1 - u2);
      if UTF8Char(u1) = #0 then
        Exit(0);
    end;
  Result := 0;
end;

