function strstr(s1, s2 : PUTF8Char):PUTF8Char;
var
  sc1, sc2: PUTF8Char ;
begin
  { find first occurrence of s2[] in s1[] }
  if s2^ = #0 then
     Exit(PUTF8Char(s1));
  s1 := strchr(s1, s2^);
  while s1 <> nil do
  begin   { match rest of prefix }

    while True do
    begin
      sc1 := s1; sc2 := s2;
      if PreInc(sc2)^ = #0  then
         Exit(PUTF8Char(s1))
      else
      if (PreInc(sc1)^ <> sc2^) then
          break;
    end;

    Inc(s1);
  end;
  Exit((0));
end;

